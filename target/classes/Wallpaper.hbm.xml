<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.yz.entity.Wallpaper" table="wallpaper" schema="wallpaper">
        <id name="id" column="id"/>
        <property name="thumbUrl" column="thumb_url"/>
        <property name="imgUrl" column="img_url"/>
        <property name="previewUrl" column="preview_url"/>
        <property name="tag" column="tag"/>
    </class>
</hibernate-mapping>